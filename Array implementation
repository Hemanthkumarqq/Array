#include <stdio.h>

#define MAX_SIZE 100 

int main() {
    int arr[MAX_SIZE]; 
    int size, i;

    printf("Enter the size of the array: ");
    scanf("%d", &size);
    if (size > MAX_SIZE || size <= 0) {
        printf("Invalid size\n");
        return 1; 
    }

    printf("Enter elements of the array:\n");
    for (i = 0; i < size; i++) {
        scanf("%d", &arr[I]);
    }

    printf("Elements of the array are:\n");
    for (i = 0; i < size; i++) {
        printf("%d ", arr[i]); 
    }
    printf("\n");

    return 0;
}
